{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.cjs"
        },
        {
            "source": "/export/(.*)",
            "destination": "/server/index.cjs"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/webhook/(.*)",
            "destination": "https://auto.mamadev.org/webhook/$1"
        }
    ]
}