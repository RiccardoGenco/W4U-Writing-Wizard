{
    "rewrites": [
        {
            "source": "/webhook/:match*",
            "destination": "https://auto.mamadev.org/webhook/:match*"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/export/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}