{
    "rewrites": [
        {
            "source": "/webhook/:match*",
            "destination": "https://auto.mamadev.org/webhook/:match*"
        },
        {
            "source": "/api/:match*",
            "destination": "/api/index.js"
        },
        {
            "source": "/export/:match*",
            "destination": "/api/index.js"
        },
        {
            "source": "/:match*",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 60
        }
    }
}