{
    "routes": [
        {
            "src": "/webhook/(.*)",
            "dest": "https://auto.mamadev.org/webhook/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.cjs"
        },
        {
            "src": "/export/(.*)",
            "dest": "/api/index.cjs"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "functions": {
        "api/index.cjs": {
            "maxDuration": 60
        }
    }
}